
public class TESTS {
	
	
	Person  [] p = {new Person("Ignacio"),
					new Person("Maverick"),
					new Person("Kevin"),
					new Person("Adonis"),
					new Person("Analepsis")
	};
	
	public void AddToEnd() {
		LIST l = new LIST();
		for(int i = 0; i<p.length; i++) {
			l.AddEnd(p[i]);
		}
		l.printInfos();
	}
	
	public void AddToStart() {
		LIST l = new LIST();
		for(int i = 0; i<p.length; i++) {
			l.AddFirst(p[i]);
		}
		l.printInfos();
	}
	
	public void length() {
		LIST l = new LIST();
		for(int i = 0; i<p.length; i++) {
			System.out.println(l.getLength() == i);
			l.AddEnd(p[i]);
		}
		
	}
	
}
